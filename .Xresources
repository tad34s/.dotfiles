


Xft.dpi: 170 
! XTerm
XTerm.termName: xterm
!! Font
xterm*faceName: Noto-Sans-Mono 
xterm*faceSize: 9
xterm*letterSpacing: 0
URxvt*font:     Noto-Sans-Mono 
URxvt*boldFont: Noto-Sans-Mono-Bold  


!! For utf-8
XTerm.vt100.locale: false
XTerm.vt100.utf8: true
!! for alt
XTerm.vt100.metaSendsEscape: true
!! for backspace
XTerm.vt100.backarrowKey: false
XTerm.ttyModes: erase ^?
!! scrollbar
XTerm.vt100.scrollBar: true
XTerm.vt100.scrollbar.width: 8
!! Urgency
XTerm.vt100.bellIsUrgent: true

! Colors
! special
*.foreground:   #d4dbff
*.background:   #121212
*.cursorColor:  #d4dbff

! black
*.color0:       #282a2e
*.color8:       #444950
*.color16:      #707880
 
! magenta
*.color1:       #D54C73
*.color9:      	#908cdb
*.color17:      #3837dd

! green
*.color2:       #78dc8d
*.color10:      #7bdb9a

! yellow
*.color3:       #fcf291
*.color11:      #f5ec9a

! blue
